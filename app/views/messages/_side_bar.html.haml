.chat-side
  .side-header
    .side-header__box
      .side-header__box__user-name
        = current_user.name
      %ul.side-header__box__icon
        .side-header__box__icon__new-group
          = link_to "#" do
            %i.fa.fa-edit
        .side-header__box__icon__edit-user    
          = link_to edit_user_path(current_user) do
            %i.fa.fa-cog      
  .group-list
    .group
      .group__group-name
        techexpert
      .group__group-message
        テストに合格しました！
